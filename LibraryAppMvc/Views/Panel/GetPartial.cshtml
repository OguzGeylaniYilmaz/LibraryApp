@using LibraryAppMvc.Models.Entity
@model List<Duyurular>

@foreach (var x in Model)
{
    <div class="post">
        <div class="user-block">
            <img class="img-circle img-bordered-sm" src="~/Admin/dist/img/4.png" alt="user image">
            <span class="username">
                <a href="#">@x.Kategori</a>
                <a href="#" class="float-right btn-tool"><i class="fas fa-times"></i></a>
            </span>
            <span class="description">@x.Tarih.ToString()</span>
        </div>
        <p>

        </p>

    </div>
}

